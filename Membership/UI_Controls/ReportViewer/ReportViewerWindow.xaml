<UserControl x:Class="Membership.UI_Controls.ReportViewer.ReportViewerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:rv="clr-namespace:Microsoft.Reporting.WinForms;assembly=Microsoft.ReportViewer.WinForms"
        mc:Ignorable="d" 
        DataContext="{Binding RelativeSource={RelativeSource Self}}"
        d:DesignHeight="400" d:DesignWidth="750">

    
    <DockPanel LastChildFill="True">
        <WindowsFormsHost DockPanel.Dock="Top" Margin="20">
            <rv:ReportViewer x:Name="ReportViewer" Visible="True"  Enabled="True" Dock="None" />
        </WindowsFormsHost>
    </DockPanel>
</UserControl>
